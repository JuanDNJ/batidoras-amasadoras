{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@config/*": ["src/config/*"],
      "@layouts/*": ["src/layouts/*"],
      "@templates/*": ["src/templates/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"]
    }
  }
}
